2nd time doing it:

#include <cs50.h>                                               //for get_float, get_int
#include <math.h>                                               //probably for round
#include <stdio.h>                                              //for printf

int main (void)
{
    float initial_amount;                                       //declare inital amount
    do
    {
        initial_amount = get_float("Please enter amount: ");
    }
    while (initial_amount < 0);                                 //prompt user (keep asking if amount is not positive)

    int round_amount = round(initial_amount * 100);             //convert to cents

    int coins = 0;                                              //initial coin counter
    while(round_amount >= 25)
    {
        round_amount -= 25;
        coins++;
    }
    while(round_amount >= 10)
    {
        round_amount -= 10;
        coins++;
    }
    while(round_amount >= 5)
    {
        round_amount -= 5;
        coins++;
    }
    while(round_amount >= 1)
    {
        round_amount -= 1;
        coins++;
    }
printf("Your min. amount of coins is: %i\n", coins);
}

--------------------------------------------------------------------------------------------------
1st time doing it:

#include <stdio.h>
#include <cs50.h>
#include <math.h>

int main(void)
{
    float dollars;
    do
    {
        dollars = get_float("Change owed: ");
    }
    while (dollars < 0);                //do-while loop to keep asking if dollar is negative

    int cents = round(dollars * 100);   //converts dollar to cent
    
    int coins = 0;                      //keep track of how many coins required
    while (cents >= 25)
    {
        cents -= 25;
        coins++;                        //when cents > 25, increase coin count by 1
    }
    while (cents >= 10)
    {
        cents -= 10;
        coins++;
    }
    while (cents >= 5)
    {
        cents -= 5;
        coins++;
    }
    while (cents >= 1)
    {
        cents -= 1;
        coins++;
    }
    printf("%i\n", coins);
}

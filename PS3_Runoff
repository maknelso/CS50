bool vote(int voter, int rank, string name)
{
    for (int i = 0, m = voter_count; i < m; i++)
    {
        for (int j = 0, n = candidate_count; j < n; j++)
        {
            if (strcmp(name, candidates[i].name) == 0)
            {
                preferences[voter][rank] = j;
                return true;

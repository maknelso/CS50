from cs50 import get_float

while True:
    amount = get_float("Change owed: ")
    if amount >= 0:
        break

cents = round(amount * 100)
counter = 0

while cents > 0:
    if (cents >= 25):
        cents -= 25
        counter += 1

    elif (cents >= 10):
        cents -= 10
        counter += 1

    elif (cents >= 5):
        cents -= 5
        counter += 1

    elif (cents >= 1):
        cents -= 1
        counter += 1

print(f"{counter}")
